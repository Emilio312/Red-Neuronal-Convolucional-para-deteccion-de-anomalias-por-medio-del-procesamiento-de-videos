   "source": [
    "import math\n",
    "\n",
    "def msf(numero):\n",
    "    parte_decimal, parte_entera = math.modf(numero)\n",
    "    decimal = 100*(round(parte_decimal,3))\n",
    "    frame = (parte_entera*60+decimal)*30\n",
    "    return frame\n",
    "\n",
    "msf(2.43)"
   ]
